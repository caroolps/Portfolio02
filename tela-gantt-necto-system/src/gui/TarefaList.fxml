         <font>
            <Font name="Ebrima" size="18.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <ToolBar prefHeight="43.0" prefWidth="1074.0">
        <items>
          <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="New" />
        </items>
      </ToolBar>
      <TableView fx:id="tableViewProjetos" prefHeight="481.0" prefWidth="1074.0">
