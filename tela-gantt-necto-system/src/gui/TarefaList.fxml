          <TableColumn fx:id="tableColumnId" prefWidth="56.0" text="Id" />
          <TableColumn fx:id="tableColumnTaskname" prefWidth="138.0" text="Task Name" />
            <TableColumn fx:id="tableColumnStarttime" prefWidth="94.0" text="Start Time" />
            <TableColumn fx:id="tableColumnOwner" prefWidth="91.0" text="Owner" />
            <TableColumn fx:id="tableColumnDuration" prefWidth="86.0" text="Duration" />
            <TableColumn fx:id="tableColumnProjeto" prefWidth="86.0" text="Projeto" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="84.0" />
